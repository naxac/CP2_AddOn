function actor_has_weapon()
	local res = false
	Actor:iterate_inventory(function(actor, item)
		if (not res) and isMagazinedWeapon(item) then
			res = true
		end
	end,
	Actor)
	return res
end
function actor_dont_has_weapon()
	return not actor_has_weapon()
end

